{
  "name": "website",
  "version": "1.0.0",
  "description": "This is a website for inGenius 2020",
  "main": "index.html",
  "scripts": {
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir=\"dist/img\"",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin gallery.html -d dist --htmlmin -o dist/gallery.html && usemin team.html -d dist --htmlmin -o dist/team.html ",
    "build": "npm run clean &&  npm run imagemin && npm run copyfonts && npm run usemin",
    "preinstall": "npx npm-force-resolutions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amitnayakblr/conFusion.git"
  },
  "author": "inGenius",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^3.0.0",
    "lite-server": "^2.3.0",
    "onchange": "^7.0.2",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "animejs": "^3.2.0",
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "install": "^0.13.0",
    "jquery": "^3.5.0",
    "npm": "^6.14.8",
    "popper.js": "^1.16.1"
  }
}
